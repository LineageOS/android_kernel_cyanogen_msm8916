############################################
## 	Kconfig for alsprox sensors devices	
																		
############################################

config YL_SENSORS_PA12200001
	tristate "TXC_PA12200001 P/L sensor support"
	default n
	depends on I2C=y
	help
	  If you say yes here you get support for TXC's proximity
	  and light sensors PA12200001.
